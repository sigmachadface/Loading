Type = 1
spawn(function()
    while wait(.1) do
        if Type == 1 then
            Pos = CFrame.new(0,PosY,0)
		elseif Type == 2 then
			Pos = CFrame.new(0,PosY,-30)
		elseif Type == 3 then
			Pos = CFrame.new(30,PosY,0)
		elseif Type == 4 then
			Pos = CFrame.new(0,PosY,30)	
		elseif Type == 5 then
			Pos = CFrame.new(-30,PosY,0)
		elseif Type == 6 then
			Pos = CFrame.new(0,PosY,-30)
        end
        end
    end)

spawn(function()
    while wait(.1) do
        Type = 1
        wait(0.3)
        Type = 2
        wait(0.3)
        Type = 3
        wait(0.3)
        Type = 4
        wait(0.3)
        Type = 5
        wait(0.3)
    end
end)



Type1 = 1
